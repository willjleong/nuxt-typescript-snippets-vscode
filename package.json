{
	"name": "nuxt-typescript-snippets",
	"repository": "https://github.com/willjleong/nuxt-typescript-snippets-vscode",
	"publisher": "willjleong",
	"displayName": "Nuxt Typescript Snippets",
	"description": "code generator & snippets for nuxt apps that utilize typescript & nuxt property decorator",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"snippets": [
			{
				"language": "vue",
				"extentions": [
					".vue"
				],
				"path": "./src/vue-snippets.json"
			},
			{
				"language": "typescript",
				"extensions": [
					".ts",
					".tsx"
				],
				"path": "./src/typescript-snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"ts-loader": "^5.3.3",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0"
	},
	"dependencies": {
		"npm": "^6.9.0"
	}
}